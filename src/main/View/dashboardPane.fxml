<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="dashboardPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #e3ecf9;"
            fx:controller="main.Controller.AdminDashboard">

    <children>
        <Label alignment="CENTER" layoutX="304.0" layoutY="14.0" minHeight="100.0" prefWidth="100.0" text="Admin Dashboard" textFill="#2a7db8" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <font>
                <Font name="Segoe UI Black" size="30.0" />
            </font>
            <effect>
                <Glow level="0.47" />
            </effect>
        </Label>
        <Separator layoutX="318.0" layoutY="99.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0" />
        <LineChart layoutX="75.0" layoutY="100.0" prefHeight="350.0" prefWidth="650.0" title="Current Month Sales" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </LineChart>
    </children>

</AnchorPane>